@font-face
  font-family "Flaticon"
  src url($PATH_FONT"/flaticon/flaticon.eot")
  src url($PATH_FONT"/flaticon/flaticon.eot#iefix") format("embedded-opentype"),
 url($PATH_FONT"/flaticon/flaticon.woff") format("woff"),
	url($PATH_FONT"/flaticon/flaticon.ttf") format("truetype"),
	url($PATH_FONT"/flaticon/flaticon.svg") format("svg")
  font-weight normal
  font-style normal

[class^="flaticon-"]:before, [class*=" flaticon-"]:before,
[class^="flaticon-"]:after, [class*=" flaticon-"]:after
  font-family Flaticon
  font-size 20px
  font-style normal
  margin-left 20px

.flaticon-facebook25
  &:before
    content "\e000"

.flaticon-pin8
  &:before
    content "\e001"

.flaticon-twitter16
  &:before
    content "\e002"

